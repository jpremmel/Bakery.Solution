@using Bakery.Models;
<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Pierre's Bakery</title>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link href="~/css/styles.css" rel="stylesheet" type="text/css">
  </head>
  <body>
    <div class="container">
      <h1>Your Vendors</h1>
        @foreach(Vendor vendor in Vendor.VendorList)
        {
          <h3>@vendor.Name</h3>

        }
      <a href="/vendors/new"><button class="btn btn-warning">Add A Vendor</button></a>
    </div>
  </body>
</html>